package ar.edu.unlp.info.oo1.ej19_envioPaquetes;

import java.time.LocalDate;

public class EnvioLocal implements Envio{

	private boolean envRapido;
	
	public double calcularPrecio() {
		if (this.envRapido) {
			return(1500);
		}
		return(1000);
	}

	public LocalDate getFecha() {
		return this.fecha;
	}
}
