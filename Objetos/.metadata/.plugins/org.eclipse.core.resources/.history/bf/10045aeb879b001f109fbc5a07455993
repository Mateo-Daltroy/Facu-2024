package ar.edu.unlp.info.oo1.ej19_envioPaquetes;

public class EnvioInternacional implements Envio {

	public double calcularPrecio() {
		if (this.peso <= 1000) {
			return(5000 + 10 * this.peso);
		} else {
			return(5000 + 12 * this.peso);
		}
	}
	
	public String getFecha() {
		return this.fecha;
	}
}
